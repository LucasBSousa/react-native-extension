{
  "name": "zeplin-react-native-extension",
  "version": "1.0.0",
  "description": "React native extension for Zeplin",
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/webpack --config webpack.prod.js",
    "clean": "rm -rf ./dist",
    "test": "npm run build:test && jest && npm run clean",
    "build:test": "npm run clean && ./node_modules/.bin/webpack --config ./webpack.tst.js",
    "lint": "node_modules/.bin/eslint src",
    "dev-server": "node_modules/.bin/webpack-dev-server --config webpack.dev.js --color",
    "sync": "./sync.sh"
  },
  "devDependencies": {
    "@zeplin/eslint-config": "^1.2.2",
    "dotenv": "^5.0.0",
    "eslint": "^4.17.0",
    "jest": "^22.1.4",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "zeplin-extension-model": "git+ssh://git@github.com/zeplin/zeplin-extension-model.git#v1.x.x"
  }
}
